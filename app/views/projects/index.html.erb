<h1>Built @ Flatiron</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Subtitle</th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name, project %></td>
    <td><%= project.subtitle %></td>
    <td><%= image_tag project.thumbnail.url(:medium) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Project', new_project_path %>
