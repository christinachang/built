<p id="notice"><%= notice %></p>

<h1><%= @project.name%></h1>

<p>
  <b>Repo Owner/Name:</b>
  <%= @project.repo_name %>
</p>

<p>
  <b>Repo URL</b>
  <a href = "<%= @project.repo_url %>" target="_blank"><%= @project.repo_url %></a>
</p>

<p>
  <b>Description:</b>
  <%= @project.description %>
</p>

<p>
  <b>Git Clone URL (SSH):</b>
  <%= @project.ssh_url %>
</p>

<p>
  <b>GitHub Forks Count:</b>
  <%= @project.forks %>
</p>

<p>
  <b>GitHub Star Count:</b>
  <%= @project.watchers %>
</p>

<p>
  <b>Technologies:</b>
  <%= @project.language %>
</p>

<p>
  <b>Project Team Members:</b>
  <% @project.users.each do |user| %>
    <a href = "<%= user.github_html_url %>" target="_blank"><%= user.full_name %></a>, 
  <%end%>
</p>
<!-- <p>
  <b>Contribs' GitHub Links:</b>
  <ul><% @project.users.each do |user| %>
  <li><a href = "<%= user.github_html_url %>">
    <%= user.github_login %></a></li>
  <%end%></ul>
</p> -->


<p>
  <b>Project Images:</b><br/>
  <% @project.images.each do |image| %>
    <%= image_tag image.upload.url(:medium) %>
  <%end%>
</p>


<%= link_to 'Edit', edit_project_path(@project) %> | <%= link_to 'Delete', @project, method: :delete, data: { confirm: 'Are you sure?' } %> |
<%= link_to 'Back', projects_path %>
