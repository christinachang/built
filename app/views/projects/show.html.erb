<div class="subnav">
  <h1 class="repo_name"><%= @project.name%></h1>
</div>
<div class="container">
  <div class="row-fluid">
<%= render :partial => 'github_info', :locals => {:project => @project} %>
  </div>
</div>
<div class="jumbotron">
    <%= render :partial =>'carousel_slideshow', :locals => {:project => @project} %>
</div>
<div class="container main">
  <div class="row-fluid">
<%= render :partial => 'collaborators_info', :locals => {:project => @project} %>
</div>

<hr/>
<div class="row-fluid">
    <h3> Analytics: </h3>
   <div id="container" class="" style="height: 300px; margin: 0 auto"></div>
 </div>

<div class="row-fluid">
  <div id="distribution_container" class=""></div>
</div>


  <%= javascript_tag do %>
  	<%= render :template => '/projects/graph.js.erb' %>
  <% end %>

  <%= javascript_tag do %>
  	<%= render :template => '/projects/distribution_graph.js.erb' %>
  <% end %>

  <div class="edit_delete">
    <% if current_user %>
    <% if current_user.is_authorized?(params) %>
    <%= link_to 'Edit', edit_project_path(@project) %> | <%= link_to 'Delete', @project, method: :delete, data: { confirm: 'Are you sure?' } %> |
    <% end %>
    <% end %>
    <%= link_to 'Back', projects_path %>
  </div>
</div>
