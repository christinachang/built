<h3> Share Contact Info: </h3>

<p><%= label_tag :full_name %> </p>
<%= f.text_field :full_name%></p>
<% if @user.errors[:full_name] %>
	<% @user.errors[:full_name].each do |message| %>	
		<h5> <%= message %> </h5>
	<% end %>
<% end %> 

<p><%= label_tag :github_html_url %>
<%= f.text_field :github_html_url %></p>


<p><%= label_tag :linkedin_url %>
<%= f.text_field :linkedin_url %></p>


<p><%= label_tag :facebook_url %>
<%= f.text_field :facebook_url %></p>
 

<p><%= label_tag :twitter_url %>
<%= f.text_field :twitter_url %></p>
