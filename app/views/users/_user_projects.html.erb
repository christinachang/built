<div class="field">
  <ul><% user.projects.each do |project| %>
      <li><% project.images.where(image_type: "cover").each do |image|%>
            <%= image_tag image.upload.url(:thumb)%>
          <%end%>
      <%= project.name %> <%= link_to 'Edit', edit_project_path(project)%></li>
      <%end%>
  </ul>
</div>
