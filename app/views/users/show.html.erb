
<div class="container">

<div id="built-main-container">

<script type="text/javascript">
  $('document').ready(function(){
    $('.tab-content').hide();
    $('.tab-content').first().show();
      $('.nav-pills li').click(function() {
        $('.nav-pills li').removeClass("active");
        $(this).addClass("active");
        $('.tab-content').hide();
        var activeTab = $(this).find("a").attr("href");
        console.log(activeTab);
        $(activeTab).show();
        });
  });

</script>



<p><%= notice %></p>

<ul class="nav nav-pills">
  <li class="active">
    <a href="#your_projects">Your Projects</a>
  </li>
  <li><a href="#submit_project">Submit a Project</a></li>
  <li><a href="#editprofile">Edit Your Profile</a></li>
</ul>

<div class="tab-content" id="your_projects">
  <%= render :partial => 'user_projects', :locals => {:user => @user} %>
</div>

<div class="tab-content" id="submit_project">
  <%= render :partial => '/projects/form', :locals => {:project => @project} %> 
</div>

<div class="tab-content" id="editprofile">
  <%= render :partial => 'contact_info', :locals => {:user => @user} %>
</div>


</div>
